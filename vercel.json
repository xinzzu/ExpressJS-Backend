{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/mahasiswa/(.*)", "dest": "/api/mahasiswarouters/$1" },
    { "src": "/api/users/(.*)", "dest": "/api/users/$1" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}
