{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/mahasiswa(.*)",
      "dest": "/api/mahasiswa$1"
    },
    {
      "src": "/api/users(.*)",
      "dest": "/api/users$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
